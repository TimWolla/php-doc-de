 <title>Die Klasse Generator</title>
 <titleabbrev>Generator</titleabbrev>

 <partintro>

<!-- {{{ Generator intro -->
  <section xml:id="generator.intro">
   &reftitle.intro;
   <para>
    <classname>Generator</classname>-Objekte werden von
    <link linkend="language.generators">Generatoren</link> zurückgegeben.
   </para>

   <caution>
    <para>
     <classname>Generator</classname>-Objekte können nicht durch
     <link linkend="language.oop5.basic.new">new</link> instanziiert werden.
    </para>
   </caution>

  </section>
<!-- }}} -->

  <section xml:id="generator.synopsis">
   &reftitle.classsynopsis;

<!-- {{{ Synopsis -->
   <classsynopsis class="class">
    <ooclass>
     <modifier>final</modifier>
     <classname>Generator</classname>
    </ooclass>

    <oointerface>
     <modifier>implements</modifier>
     <interfacename>Iterator</interfacename>
    </oointerface>

    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
    <xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.generator')/db:refentry/db:refsect1[@role='description']/descendant::db:methodsynopsis[@role='Generator'])">
     <xi:fallback/>
    </xi:include>
   </classsynopsis>
<!-- }}} -->

  </section>

  <section xml:id="generator.seealso">
   &reftitle.seealso;
   <para>Siehe <link linkend="language.oop5.iterations">Objekt-Iteration</link>.</para>
  </section>

